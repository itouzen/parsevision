{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit (Parse Vision)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/index.py",
            "module": "streamlit",
            "args": [
                "run",
                "index.py",
                "--server.port",
                "2000"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}
